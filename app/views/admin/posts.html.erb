<main role="main" class="col-sm-9 ml-sm-auto col-md-10 pt-3">
     <h2>게시판 관리</h2>
     <h3>글 목록</h3>
     <div class="table-responsive">
       <table class="table table-striped">
         <thead>
           <tr>
             <th>#</th>
             <th>Title</th>
             <th>Content</th>
             <th>Image</th>
             <th>Status</th>
           </tr>
         </thead>
         <tbody>
            <%@posts.each do |post| %>
           <tr>
             <td><%=post.id %></td>
             <td><%=post.title %></td>
             <td><%=post.content %></td>
             <td><img src= "<%=post.image.thumb%>"></td>
             <td>
                <%= link_to "삭제", "/admin/posts_destroy/#{post.id}", class: 'btn btn-primary', method: :put, data:{confirm: "정말로 삭제하시겠습니까?"} %>
             </td>
           </tr>
           <% end %>
         </tbody>
       </table>
     </div>
   </main>
 </div>
</div>
